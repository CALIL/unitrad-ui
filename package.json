    "babelify": "^10.0.0",
    "browser-sync": "^2.26.7",
    "browserify": "^16.5.0",
    "bundle-collapser": "^1.4.0",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "cssnano": "^4.1.10",
    "ejs": "^3.0.1",
