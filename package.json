    "react-paginate": "6.3.2",
    "superagent": "^5.2.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "node",
      "react",
      "flowtype"
    ],
    "ecmaFeatures": {
      "jsx": true,
