{
  "name": "unitrad-ui",
  "version": "1.8.0",
  "description": "Standard web interface for the Library federated catalog.",
  "main": "index.html",
  "scripts": {
    "flow": "flow",
    "start": "gulp debug --conf=./conf/",
    "release": "gulp release",
    "test": "mocha --require @babel/register",
    "test-with-browser": "gulp test",
    "preinstall": "npx npm-force-resolutions"
  },
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-flow-strip-types": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/register": "^7.12.1",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "babelify": "^10.0.0",
    "browser-sync": "^2.27.11",
    "browserify": "^17.0.0",
    "bundle-collapser": "^1.4.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "cssnano": "^4.1.10",
    "ejs": "^3.1.7",
    "eslint": "^7.12.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "flow-bin": "^0.136.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-ejs": "^5.1.0",
    "gulp-header": "^2.0.9",
    "gulp-mocha": "^8.0.0",
    "gulp-postcss": "^9.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "licensify": "^3.1.3",
    "minimist": "^1.2.5",
    "mocha": "^8.2.0",
    "mocha-junit-reporter": "^2.0.0",
    "postcss": "^8.2.13",
    "through2": "^4.0.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CALIL/unitrad-ui"
  },
  "author": "CALIL Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CALIL/unitrad-ui/issues"
  },
  "homepage": "https://github.com/CALIL/unitrad-ui#readme",
  "dependencies": {
    "core-js": "^3.6.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-paginate": "6.5.0",
    "superagent": "^6.1.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "node",
      "react",
      "flowtype"
    ],
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "browser": true
    }
  },
  "resolutions": {
    "minimist": "^1.2.5"
  }
}
